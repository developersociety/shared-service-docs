{
  "name": "openresources/resourcehub-distribution",
  "description": "Resource Hub distribution",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "homepage": "https://gitlab.com/openresources/resourcehub_distribution",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "ckeditor/balloonpanel": "^4.0",
    "ckeditor/a11ychecker": "^1.0",
    "drupal/admin_toolbar": "^3.0",
    "drupal/allowed_formats": "^1.3",
    "drupal/core": "^9.1",
    "drupal/ckeditor_a11ychecker": "1.x-dev@dev",
    "drupal/ckeditor_balloonpanel": "^1.1",
    "drupal/crop": "^2.1",
    "drupal/default_content": "^2.0@alpha",
    "drupal/extlink": "^1.6",
    "drupal/facets": "^1.7",
    "drupal/field_group": "^3.1",
    "drupal/gin": "^3.0@alpha",
    "drupal/gin_toolbar": "^1.0@beta",
    "drupal/google_tag": "^1.3",
    "drupal/image_widget_crop": "^2.3",
    "drupal/linkit": "^6.0@beta",
    "drupal/oembed_providers": "^1.1",
    "drupal/masquerade": "^2.0@beta",
    "drupal/metatag": "^1.16",
    "drupal/menu_admin_per_menu": "^1.3",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.6",
    "drupal/redirect": "^1.5",
    "drupal/responsive_menu": "^4.0",
    "drupal/role_delegation": "^1.1",
    "drupal/shariff": "^1.7",
    "drupal/search_api": "^1.19",
    "drupal/simple_sitemap": "^3.9",
    "drupal/simplify": "^1.2",
    "drupal/styleguide": "^2.0@beta",
    "drupal/view_unpublished": "1.0",
    "drupal/entity_reference_facet_link": "^1.0@beta",
    "bower-asset/mmenu": "^8.5.24",
    "npm-asset/shariff": "^3.2.1",
    "npm-asset/superfish": "^1.7.10",
    "openresources/resourcehub_rest": "^1.0@alpha"
  },
  "replace": {
    "npm-asset/fortawesome--fontawesome-free": "*",
    "npm-asset/jquery": "*"
  },
  "require-dev": {
    "drupal/core-dev": "^9.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": { }
  }
}
